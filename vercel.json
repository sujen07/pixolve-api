{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "build": {
    "env": {
      "CMAKE_GENERATOR": "Unix Makefiles"
    },
    "commands": [
      "apt-get update && apt-get install -y cmake",
      "pip install -r requirements.txt"
    ]
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
